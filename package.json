{
  "name": "nextjs-apollo-ts",
  "version": "1.0.0",
  "license": "MIT",
  "description": "nextjs, apollo/graphql, gencode, typescript... (for develop web)",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "gen:types": "apollo client:codegen --target=typescript --outputFlat src/graphql/__types --excludes **/__types --includes **/*.graphql.ts",
    "gen:types2": "apollo client:codegen --endpoint=https://api.spacex.land/graphql --target=typescript --outputFlat src/graphql/__types --excludes **/__types --includes **/*.graphql.ts"
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "apollo-link-context": "1.0.20",
